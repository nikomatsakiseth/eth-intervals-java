package lower;

class LowerForEach {
    
    sumObjects(Iterable[T: Object] list) {
        result = 0;
        list forEach { value ->
            result = result + value; // ERROR no.applicable.methods(list:T)
        };
        result
    }

    sumIntegers(Iterable[T: Integer] list) {
        result = 0;
        list forEach { value ->
            result = result + value;
        };
        result
    }
    
}
