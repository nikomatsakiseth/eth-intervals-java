# One step compilation

.PHONY: help clean

help:
	@echo "The only target is 'clean'"

clean:
	@echo Removing assembly and object files etc. 
	@rm -rf `find . -name '*.s'`
	@rm -rf `find . -name '*.o'`
	@rm -rf `find . -name '*.ps'`
	@rm -rf `find . -name '*.dot'`
