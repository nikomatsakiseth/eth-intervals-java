package test.bytecode;

class BytecodeFields extends Application {
    
    value: Integer = null;
    
    @Override main(ctx: ApplicationContext): Void {
        value = 22;
        
        System.out.println(value);
        System.out.println(this.value);
        
        value = value + 1;
        
        System.out.println(value);
        System.out.println(this.value);
    }
    
}
