package lower;

class LowerReflection {
    invokeToString(obj: Object) {
        obj.toString().concat("foo");
    }
    
    invokeHashCode(obj: Object) {
        obj.hashCode();
    }
    
    errors(obj: Object): Void {
        invokeToString(obj).badMethod(); // ERROR no.such.method(java.lang.String, badMethod())
        invokeHashCode(obj).badMethod(); // ERROR no.such.method(java.lang.Integer, badMethod())
    }

}
