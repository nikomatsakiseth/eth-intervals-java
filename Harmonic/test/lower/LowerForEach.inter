package lower;

class LowerForEach {

    sumObjects(list: Iterable[T: Object]) {
        result = 0;
        list.forEach { value ->
            result = result + value; // ERROR no.applicable.methods(list:T)
        };
        result
    }

    sumIntegers(list: Iterable[T: Integer]) {
        result = 0;
        list.forEach { value ->
            result = result + value;
        };
        result
    }
    
    sumPairs(list: Iterable[T: (Integer, Integer)]) {
        result = 0;
        list.forEach { (x, y) ->
            result = result + (x + y);
        };
        result
    }
    
}
